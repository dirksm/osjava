<?xml version="1.0" encoding="ISO-8859-1"?>

<renderers>
 <renderer name="txt" label="Text file" class="org.osjava.reportrunner.renderers.TextRenderer">
   <inline>false</inline>
   <extension>txt</extension>
   <mimeType>text/plain</mimeType>
 </renderer>
 <renderer name="html" label="Web page" class="org.osjava.reportrunner.renderers.HtmlTableRenderer">
   <inline>true</inline>
   <extension>html</extension>
   <mimeType>text/html</mimeType>
 </renderer>
 <renderer name="tdf" label="Tab delimited" class="org.osjava.reportrunner.renderers.CsvRenderer">
   <inline>false</inline>
   <extension>csv</extension>
   <fieldDelimiter>	</fieldDelimiter>
   <mimeType>text/csv</mimeType>
 </renderer>
 <renderer name="csv" label="Comma delimited" class="org.osjava.reportrunner.renderers.CsvRenderer">
   <inline>false</inline>
   <extension>csv</extension>
   <fieldDelimiter>,</fieldDelimiter>
   <mimeType>text/csv</mimeType>
 </renderer>
 <renderer name="excel" label="Excel file" class="org.osjava.reportrunner.renderers.ExcelRenderer">
   <inline>false</inline>
   <extension>xls</extension>
   <mimeType>application/excel</mimeType>
 </renderer>

 <!-- plugin renderers -->
 <renderer name="datetime-chart" label="Time chart" class="org.osjava.reportrunner_plugins.renderers.TimeGraphRenderer">
   <inline>true</inline>
   <extension>png</extension>
   <mimeType>image/png</mimeType>
 </renderer>

</renderers>
